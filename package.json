{
  "name": "@processway/contentful-migrate-content",
  "version": "1.0.3",
  "description": "Contentful migration content",
  "homepage": "https://github.com/processway/contentful-migrate-content/blob/master/README.md",
  "author": "Oscar D. Rend√≥n L. | Processway Technology",
  "license": "ISC",
  "bin": {
    "cont": "dist/cli.js"
  },
  "main": "./dist/cli.js",
  "scripts": {
    "clean": "rm -rf node_modules differenceContent",
    "build": "tsc -p .",
    "develop": "npm link && tsc --watch",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^5.1.0",
    "contentful-cli": "^1.5.35",
    "contentful-import": "^7.9.20",
    "contentful-management": "^7.9.0",
    "contentful-migration": "^4.0.9",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.151",
    "@types/node": "^14.0.3",
    "jest": "^26.6.3",
    "typescript": "^3.9.2"
  },
  "files": [
    "dist",
    "differenceContent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/processway/contentful-migrate-content.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "jest": {
    "testTimeout": 30000,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "bugs": {
    "url": "https://github.com/processway/contentful-migrate-content/issues"
  },
  "keywords": [
    "contentful",
    "contentful-migrate"
  ]
}
